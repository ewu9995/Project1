public class JLaulib {
	public static void multiplication_Table (int base, int range)
	{
		for (int i = 0; i <= range; i++)
		{
			int multiplied_num = base * i;
			System.out.println(base + "*" + i + "=" + multiplied_num);
		}
	}
	public static void main(String[] args)
	{
		multiplication_Table(8,9);
	}

}


	public static int SumOfAll(int integer)
	{
		int sum = 0;
		for(int i = 0; i <= integer; i++)
		{
			sum = sum + i;
		}
		return sum;
	}
	public static void main(String[] args)
	{
		int Sum_of_all = 0;
		Sum_of_all = SumOfAll(100);
		System.out.println(Sum_of_all);
	}

}

